{
  "name": "geekconsole",
  "version": "0.0.2",
  "description": "",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:tailwindcss": "tailwindcss -i ./app/core/styles/index.css --minify -o ./app/styles.css",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:tailwindcss": "tailwindcss -i ./app/core/styles/index.css -o ./app/styles.css --watch",
    "typecheck": "npx tsc",
    "lint:ts": "npx eslint . --ext  .ts,.tsx ./app",
    "lint:ts:fix": "npx eslint . --ext .ts,.tsx ./app --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "format": "npx prettier --write ./app/**/*.{js,jsx,ts,tsx}",
    "format:check": "npx prettier --check ./app/**/*.{js,jsx,ts,tsx}",
    "release": "standard-version --no-verify",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/css-bundle": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@supabase/auth-helpers-remix": "^0.2.1",
    "@supabase/supabase-js": "^2.24.0",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.7",
    "isbot": "^3.6.10",
    "lucide-react": "^0.241.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.13.1",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-webpack5": "^7.0.20",
    "@storybook/testing-library": "0.1.0",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.1",
