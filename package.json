		"test": "playwright test",
		"typecheck": "tsc",
		"lint:ts": "npx eslint --no-error-on-unmatched-pattern . --ext .ts,.tsx ./app",
		"lint:ts:fix": "npx eslint --no-error-on-unmatched-pattern . --ext .ts,.tsx ./app --fix",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"format": "npx prettier --no-error-on-unmatched-pattern --write ./app/**/*.{js,jsx,ts,tsx}",
		"format:check": "npx prettier --no-error-on-unmatched-pattern --check ./app/**/*.{js,jsx,ts,tsx}",
